<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.biz.practice.dao.IProvinceDao">
    <select id="selectAll" resultType="Province">
        select *
        from provinces
    </select>
    <select id="selectByPid" resultType="Province">
        select *
        from provinces
        where pid = #{pid}
    </select>
</mapper>