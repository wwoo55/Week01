<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="priv.xy.week01.mapper.ICityMapper">
    <select id="selectByPid" parameterType="Long" resultType="City">
        select *
            from cities
            where pid = #{pid}
    </select>
    <select id="selectByCid" resultType="priv.xy.week01.domain.City">
        select *
        from cities
        where cid = #{cid}
    </select>
</mapper>